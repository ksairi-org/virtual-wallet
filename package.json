{
  "name": "virtual-wallet",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "yarn generate:hooks && yarn expo start",
    "config-envs": "doppler run -p mobile -c $0 -- doppler secrets substitute env.template.yaml --output .env",
    "dev-client-ios": "yarn config-envs $0 && dotenv -e .env -- eas build --platform ios --profile dev-client-ios --local",
    "dev-client-android": "yarn config-envs $0 && dotenv -e .env -- eas build --platform android --profile dev-client-android --local",
    "dev-client-ios-device": "yarn config-envs $0 && dotenv -e .env -- eas build --platform ios --profile dev-client-ios-real-device --local",
    "build-store-ios": "yarn config-envs $0 && dotenv -e .env -- eas build --platform ios --profile store --local --non-interactive --output ./app-build.ipa",
    "build-store-android": "yarn config-envs $0 && dotenv -e .env -- eas build --platform android --profile store --local --non-interactive --output ./app-build.aab",
    "build-store-all": "yarn build-store-android $0 && yarn build-store-ios $0",
    "deploy-store-ios": "yarn build-store-ios $0 && eas submit --platform ios --profile $0  --path ./app-build.ipa",
    "deploy-store-android": "yarn build-store-android $0 && eas submit --platform android --profile $0  --path ./app-build.aab",
    "deploy-store-all": "yarn deploy-store-android $0 && yarn deploy-store-ios $0",
    "postinstall": "husky",
    "lint": "expo lint --fix",
    "check:tsc": "tsc --noEmit",
    "generate:spec": "node libs/react-query-sdk/download-spec.js",
    "sync-be-types": "supabase gen types typescript --project-id icftqbzrrsqwzsuqgmxd --schema virtual_wallet_db_$0  > src/backend/database.types.ts",
    "generate:hooks": "yarn generate:spec && yarn orval --config libs/react-query-sdk/orval.config.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.3",
    "@react-native-community/hooks": "^100.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "22.2.0",
    "@react-native-firebase/app": "22.2.0",
    "@react-native-firebase/in-app-messaging": "22.2.0",
    "@react-native-firebase/messaging": "22.2.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tamagui/config": "^1.132.15",
    "@tanstack/react-query": "^5.83.0",
    "aes-js": "^3.1.2",
    "axios": "^1.11.0",
    "burnt": "^0.13.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "rive-react-native": "9.3.4",
    "tamagui": "1.132.15",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@phenomnomnominal/tsquery": "^6.1.3",
    "@tamagui/babel-plugin": "1.132.15",
    "@types/aes-js": "^3",
    "@types/inflected": "^2",
    "@types/react": "~19.0.10",
    "@types/react-dom": "^19",
    "@types/react-native-get-random-values": "^1",
    "@types/uuid": "^10.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "~13.0.0",
    "dotenv-cli": "^9.0.0",
    "eas-cli": "16.17.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "inflected": "^2.1.0",
    "orval": "^7.11.2",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.1",
    "supabase": "2.34.3",
    "typescript": "~5.8.3"
  },
  "packageManager": "yarn@4.9.2",
  "private": true
}
