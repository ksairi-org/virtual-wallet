{
  "name": "virtual-wallet",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "pre-start": "doppler run -p mobile -c $0 --command \"doppler secrets substitute env.template.yaml --output .env; source .env; yarn generate:open-api-hooks; $1\"",
    "start:expo": "[ $0 == 'prod' ] && yarn expo start --scheme virtual-wallet || yarn expo start --scheme virtual-wallet-$0",
    "start": "yarn i18n-compile && yarn pre-start ${0:-stg} \"yarn start:expo ${0:-stg}\"",
    "build": "doppler run -p mobile -c $0 --command \"doppler secrets substitute env.template.yaml --output .env; source .env; yarn generate:open-api-hooks; $1\"",
    "pre-build": "yarn i18n-compile && yarn build $0 \"$1\"",
    "dev-client-ios": "yarn pre-build $0 \"eas build --platform ios --profile dev-client-ios --local\"",
    "dev-client-android": "yarn pre-build $0 \"eas build --platform android --profile dev-client-android --local\"",
    "dev-client-ios-device": "yarn pre-build $0 \"eas build --platform ios --profile dev-client-ios-real-device --local\"",
    "build-store-ios": "yarn pre-build $0 \"eas build --platform ios --profile store --local --non-interactive --output ./app-build.ipa\"",
    "build-store-android": "yarn pre-build $0 \"eas build --platform android --profile store --local --non-interactive --output ./app-build.aab\"",
    "build-store-all": "yarn build-store-android $0 && yarn build-store-ios $0",
    "deploy-store-ios": "yarn build-store-ios $0 && eas submit --platform ios --profile $0  --path ./app-build.ipa",
    "deploy-store-android": "yarn build-store-android $0 && eas submit --platform android --profile $0  --path ./app-build.aab",
    "deploy-store-all": "yarn deploy-store-android $0 && yarn deploy-store-ios $0",
    "postinstall": "husky",
    "lint": "expo lint --fix",
    "check:tsc": "tsc --noEmit",
    "generate:open-api-spec": "node libs/react-query-sdk/scripts/generate-open-api-spec.js",
    "generate:open-api-hooks": "yarn generate:open-api-spec && yarn orval --config libs/react-query-sdk/orval.config.ts",
    "deploy-supabase-functions-stg": "yarn supabase functions deploy --project-ref oidpaoqsngnydihqkgok",
    "deploy-supabase-functions-prod": "yarn supabase functions deploy --project-ref yvswqpsxautkpsqgzstc",
    "i18n-extract": "lingui extract --config=src/i18n/config/lingui.config.ts --clean --overwrite",
    "i18n-compile": "yarn i18n-extract && lingui compile --config=src/i18n/config/lingui.config.ts --typescript"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@lingui/core": "^5.4.1",
    "@lingui/react": "^5.4.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/hooks": "^100.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/analytics": "23.4.0",
    "@react-native-firebase/app": "23.4.0",
    "@react-native-firebase/messaging": "23.4.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@sentry/react-native": "~7.1.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tamagui/config": "1.35.0",
    "@tanstack/react-query": "^5.83.0",
    "aes-js": "^3.1.2",
    "axios": "^1.11.0",
    "burnt": "^0.13.0",
    "date-fns": "^4.1.0",
    "expo": "54.0.12",
    "expo-auth-session": "~7.0.8",
    "expo-build-properties": "~1.0.9",
    "expo-dev-client": "~6.0.13",
    "expo-font": "~14.0.8",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.3.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-worklets": "0.5.1",
    "rive-react-native": "9.6.1",
    "tamagui": "1.135.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@lingui/babel-plugin-lingui-macro": "^5.4.1",
    "@lingui/cli": "^5.4.1",
    "@lingui/conf": "^5.4.1",
    "@phenomnomnominal/tsquery": "^6.1.3",
    "@tamagui/babel-plugin": "1.135.0",
    "@types/aes-js": "^3",
    "@types/inflected": "^2",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "@types/react-native-get-random-values": "^1",
    "@types/uuid": "^10.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "~54.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "inflected": "^2.1.0",
    "orval": "^7.11.2",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.1",
    "supabase": "2.39.2",
    "typescript": "~5.9.2"
  },
  "packageManager": "yarn@4.9.2",
  "private": true
}
