{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@default-tamagui-config": ["src/theme/tamagui.config.ts"],
      "@screens": ["src/screens"],
      "@navigation": ["src/navigation"],
      "@navigation/types": ["src/navigation/types"],
      "@utils": ["src/utils"],
      "@atoms": ["src/components/atoms"],
      "@molecules": ["src/components/molecules"],
      "@organisms": ["src/components/organisms"],
      "@theme": ["src/theme/index.ts"],
      "@sentry": ["src/services/sentry/index.ts"],
      "@firebase-messaging": ["src/services/firebase-messaging/index.ts"],
      "@react-native-hooks": ["libs/react-native-hooks"],
      "@react-hooks": ["libs/react-hooks"],
      "@react-native-functions": ["libs/react-native-functions"],
      "@react-native-ui-config": ["libs/react-native-ui-config"],
      "@typescript-functions": ["libs/typescript-functions"],
      "@react-native-splash-view": ["libs/react-native-splash-view"],
      "@ui-containers": ["libs/ui-containers"],
      "@ui-touchables": ["libs/ui-touchables"],
      "@fonts": ["src/components/atoms/fonts"],
      "@icons": ["src/components/atoms/icons"],
      "@images": ["src/components/atoms/images"],
      "@stores": ["src/stores/index.ts"],
      "@react-auth-core": ["libs/react-auth-core"],
      "@react-auth-storage": ["libs/react-auth-storage"],
      "@react-native-auth-google": ["libs/react-native-auth-google"],
      "@react-native-auth-apple": ["libs/react-native-auth-apple"],
      "@react-form": ["libs/react-form"],
      "@expo-image": ["libs/expo-image"],
      "@backend": ["src/backend"],
      "@constants": ["src/constants"],
      "@hooks": ["src/hooks"]
    }
  }
}
